{
  "name": "carbon-angular-starter",
  "version": "0.1.0",
  "license": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test:once": "ng test -- --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "cp ./src/environments/environment_prod.ts ./src/environments/environment.ts && ./node_modules/@angular/cli/bin/ng build --base-href=/procurement/serviceproc/  --prod --build-optimizer && npm run post-build",
    "build-test": "cp ./src/environments/environment_test.ts ./src/environments/environment.ts && ./node_modules/@angular/cli/bin/ng build --base-href=/procurement/serviceproc/  --prod --build-optimizer && npm run post-build",
    "build-dev": "./node_modules/@angular/cli/bin/ng build --base-href=/procurement/serviceproc/  --prod --build-optimizer  && npm run post-build",
    "production": "node ./bin/server",
    "test": "node ./bin/server",
    "dev": "node-contrast --application.name=\"Contingent Labour Platform\"  --application.group=495825  ./bin/server",
    "post-build": "npm run add_date && node ./build/post-build.js && cp -r ./src/robots-blocked.txt ./dist/robots-blocked.txt",
    "add_date": "npm run add_date_js && npm run add_date_css && npm run rm_bak_files",
    "add_date_js": "for i in dist/procurement/serviceproc/*; do if [ -f $i ]; then LC_ALL=C sed -i.bak 's:js\":js?'$(date +%H%M%m%d%y)'\":g' $i; fi done",
    "add_date_css": "sed -i.bak 's:css\":css?'$(date +%H%M%m%d%y)'\":g' dist/procurement/serviceproc/index.html",
    "rm_bak_files": "find dist -name '*.bak' -exec rm -Rf {} \\;"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "^8.2.8",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^10.1.3",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@angular/router": "^8.2.8",
    "@carbon/charts": "0.41.65",
    "@carbon/charts-angular": "0.41.65",
    "@carbon/icons-angular": "^10.8.0",
    "@carbon/telemetry": "0.0.0-alpha.6",
    "@carbon/type": "^10.28.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@ng-idle/core": "^8.0.0-beta.4",
    "@ngx-translate/core": "^12.1.2",
    "carbon-components": "^10.6.2",
